<%
var targetUrl = project.code_link || project.live_link || '';
%>


	<div class="col-12 pb-5 pt-5 border border-left-0 border-right-0 border-top-0 border-light">
		<div class="row white">
			<div class="col-12 col-md-4 p-0">
				<a class="" href="<%= targetUrl %>">
					<img class="bg-secondary img-fluid w-100 h-100" src="<%= project.images[0] %>">
				</a>
			</div>
			<div class="col-12 col-md-8 p-3">
				<div class="row">
					<div class="col">
						<div class="row">
							<div class="col">
								<h4>
									<a class="" href="<%= targetUrl %>">
										<%= project.title %>
									</a>
								</h4>
							</div>
							<div class="col-12 col-md-2">
									<a class="teal-text mr-3" href="<%= project.live_link %>" target="__blank">
										<%- theme.icons.view %>
									</a>
									<a class="teal-text  pull-right" href="<%= project.code_link %>" target="__blank">
										<%- theme.icons.github %>
									</a>
							</div>
						</div>
						<hr>
					</div>
				</div>
				<div class="row">
					<div class="col">
						<div class="">
							<%= project.summery %>
						</div>
						<div class="pt-3 pb-3">
							<% project.details.forEach(function(d){ %>
								<%  Object.keys(d).forEach(function(key){ %>
									<div class="small">
										<span class="text-secondary">
											<%= key %>
										</span>	
										<span>
											<%= d[key] %>
										</span>

										
									</div>
								<%  }) %>
							<% }) %>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col">
						
					</div>
				</div>
	
			</div>
		</div>
	</div>
